{
    "manifest_version": 3,
    "name": "Fortify WebInspect Scanner",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/fortify16.png",
        "48": "images/fortify48.png",
        "128": "images/fortify128.png"
      }
    },
    "icons": {
      "16": "images/fortify16.png",
      "48": "images/fortify48.png",
      "128": "images/fortify128.png"
    },
    "background": {
      "service_worker": "popup.js"
    }
  }
  